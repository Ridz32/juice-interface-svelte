{
	"name": "juicebox-v2",
	"version": "0.2.0",
	"type": "module",
	"engines": {
		"node": ">=16.0.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{ts,svelte,html,json}": [
			"prettier --write",
			"git add"
		]
	},
	"scripts": {
		"dev": "svelte-kit dev --port 8080",
		"build": "svelte-kit build && node scripts/ipfs-fallback.js",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"i18n:compile": "lingui compile",
		"ipfs-fallback": "node scripts/ipfs-fallback.js"
	},
	"devDependencies": {
		"@ethersproject/bignumber": "^5.6.0",
		"@ethersproject/contracts": "^5.6.0",
		"@jbx-protocol/contracts-v2": "^4.0.0",
		"@lingui/cli": "^3.13.3",
		"@lingui/core": "^3.13.3",
		"@playwright/test": "^1.20.0",
		"@sveltejs/adapter-static": "^1.0.0-next.29",
		"@sveltejs/kit": "^1.0.0-next.324",
		"@types/cookie": "^0.4.1",
		"@types/d3": "^7.1.0",
		"babel-plugin-macros": "^3.1.0",
		"csstype": "^3.0.11",
		"lingui_core": "^3.13.4-1",
		"pinata_ipfs_sdk": "1.1.25",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"svelte": "^3.46.0",
		"svelte-check": "^2.2.6",
		"svelte-feather-icons": "^4.0.0",
		"svelte-i18n": "^3.4.0",
		"svelte-preprocess": "^4.10.1",
		"svelte-range-slider-pips": "^2.0.3",
		"tslib": "^2.3.1",
		"typescript": "~4.6.2"
	},
	"dependencies": {
		"@fontsource/fira-mono": "^4.5.0",
		"@galtproject/browser-ipfs": "^0.5.4",
		"@lingui/detect-locale": "^3.13.3",
		"@lingui/macro": "^3.13.3",
		"@lukeed/uuid": "^2.0.0",
		"axios": "^0.27.2",
		"buffer": "^6.0.3",
		"chart.js": "^3.7.1",
		"cookie": "^0.4.1",
		"d3": "^7.4.4",
		"date-fns": "^2.28.0",
		"ethers": "^5.6.2",
		"firebase": "^9.6.10",
		"husky": "^8.0.1",
		"ipfs-core": "^0.14.2",
		"lint-staged": "^12.4.1",
		"sass": "^1.51.0",
		"svelte-apexcharts": "^1.0.2",
		"svelte-file-dropzone": "^1.0.0-dev.1",
		"walletconnect": "^1.7.7"
	}
}
